generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model reportes {
  id                Int       @id @default(autoincrement())
  tiempoSinServicio Int?
  diaDeCorte        String?   @db.VarChar(255)
  codigoPostal      Int?
  timestamp         DateTime? @default(now()) @db.Timestamp(0)
}
